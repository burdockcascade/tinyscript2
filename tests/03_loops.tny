function main() {

    test_for_loop();
    test_while_loop();
    test_for_in_loop();

}

function test_for_loop() {

    // for loop
    for (var t = 3 + 1; t < 10; t = t + 1) {
        assert t >= 4;
        assert t < 10;
    }

}

function test_while_loop() {

    var b = 0;

    // while loop
    while (b < 10) {
        assert b >= 0;
        assert b < 10;
        b = b + 1;
    }
}

function test_for_in_loop() {

    var c = [2,3,4,5,6,7,8,9];

    // for in loop
    for (x in c) {
        assert x > 1;
        assert x < 10;
    }

}


