function main() {

    for (var t = 3 + 1; t < 10; t = t + 1) {
        assert t >= 4;
        assert t < 10;
    }

    var b = 0;

    while (b < 10) {
        assert b >= 0;
        assert b < 10;
        b = b + 1;
    }

    var c = [2,3,4,5,6,7,8,9];

    for (x in c) {
        assert x > 1;
        assert x < 10;
    }

}

