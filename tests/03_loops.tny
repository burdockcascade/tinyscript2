function main()
    var a = 0

    while a <= 100 do
        assert a <= 100
        assert a >= 0
        a = a + 1
    end

    assert a == 101

    var b = [1,2,4,5,7,8,9]
    var b_count = 0

    for item in b do
        assert true
        assert item >= 1
        assert item <= 9
        b_count = b_count + 1
    end

    assert b_count == 7

    for i = 2 to 12 step 2 do
        assert i >= 2
        assert i <= 12
    end
end