class person {

    var name;
    var age;

    function constructor() {
        this.name = "bob";
        this.age = 99;
    }

    function greet() {
        print "hello, my name is " + this.name + " and I am " + this.age + " years old.";
    }

    function speak(msg) {
        print msg;
    }
}

function main(argv) {

    var man = new person();

    man.greet();

}