class main {

    function i_do_nothing() {
        assert false;
    }

    function test_parameter_order(bool, ignore_this_param) {
        assert bool;
    }

    function i_return_what_you_give_me(b) {
        return b;
    }

    function main(argv) {
        print("hello world");

        assert main.i_return_what_you_give_me(true);

        var vars = new variables();
        vars.test_strings();
        vars.test_numbers();
        vars.test_floats();
        vars.test_booleans();
        vars.test_math();

    }

}

class functions {

    function test_functions() {

        var lp = new loops();
        var count = do_loopy([1,2,4,5,7,8,9]);
        assert count == 7;

        func1(true, false);

        var f = function(bool) {
            assert bool;
        };

        f(true);

        var x = f;
        x(true);

        execute(f);
        execute(x);

        execute(function(b3) {
            assert true;
        });

        assert i_return_what_you_give_me(true);

        var table = {
            "f1": function(b5) {
                assert b5;
            }
        };

        var func1 = table["f1"];
        func1(true);

    }

    function loopy(b) {
        var b_count = 0;

        for (item in b) {
            assert item <= 9;
            assert item >= 1;
            b_count = b_count + 1;
        }

        return b_count;
    }
}

class variables {

    function test_strings() {

        var myFirstName = "Sanela";
        assert myFirstName == "Sanela";

        var myLastName = "Kurtovic";
        assert myLastName == "Kurtovic";

        var myHomeTown = "Sarajevo";
        assert myHomeTown == "Sarajevo";

        var myAge = 30;
        assert myAge == 30;

        var myFullName = myFirstName + " " + myLastName;
        assert myFullName == "Sanela Kurtovic";

        var myStory = "My name is " + myFullName + " and I am " + myAge + " years old. I was born in " + myHomeTown + " and I really like it there.";
        assert myStory == "My name is Sanela Kurtovic and I am 30 years old. I was born in Sarajevo and I really like it there.";

    }

    function test_numbers() {

        var number = 5;
        assert number == 5;

        var number2 = 10;
        assert number2 == 10;

        var number3 = 15;
        assert number3 == 15;

        var number4 = 20;
        assert number4 == 20;

        var number5 = 25;
        assert number5 == 25;

        var sum = number + number2 + number3 + number4 + number5;
        assert sum == 75;

        var average = sum / 5;
        assert average == 15;

    }

    function test_floats() {

        var float = 3.14;
        assert float == 3.14;

        var float2 = 5.5;
        assert float2 == 5.5;

        var float3 = 7.7;
        assert float3 == 7.7;

        var float4 = 9.9;
        assert float4 == 9.9;

        var float5 = 11.11;
        assert float5 == 11.11;

        var floatSum = float + float2 + float3 + float4 + float5;
        assert floatSum == 37.35;

        var floatAverage = floatSum / 5;
        assert floatAverage == 7.47;

    }

    function test_booleans() {

        var boolean1 = true;
        assert boolean1 == true;

        var boolean2 = false;
        assert boolean2 == false;

        var boolean3 = true;
        assert boolean3 == true;

        var boolean4 = false;
        assert boolean4 == false;

        var boolean5 = true;
        assert boolean5 == true;

        var booleanSum = boolean1 + boolean2 + boolean3 + boolean4 + boolean5;
        assert booleanSum == false;

    }

    function test_math() {

        var math = 5 + 4 * 3;
        assert math == 17;

    }
}

class loops {

    function test_for_loop() {

        // for loop
        for (var t = 3 + 1; t < 10; t = t + 1) {
            assert t >= 4;
            assert t < 10;
        }

    }

    function test_while_loop() {

        var b = 0;

        // while loop
        while (b < 10) {
            assert b >= 0;
            assert b < 10;
            b = b + 1;
        }
    }

    function test_for_in_loop() {

        var c = [2,3,4,5,6,7,8,9];

        // for in loop
        for (x in c) {
            assert x > 1;
            assert x < 10;
        }

    }
}

class person {

    var name;
    var age;

    function constructor() {
        this.name = "bob";
        this.age = 99;
    }

    function greet() {
        print("hello, my name is " + this.name + " and I am " + this.age + " years old.");
    }

    function speak(msg) {
        print(msg);
    }
}

