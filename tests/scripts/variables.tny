class Test {
    function main() {
        var v = new Variables();
        assert v != null;
    }
}

class Variables {

    function constructor() {
        assert test_strings();
        assert test_numbers();
        assert test_floats();
        assert test_booleans();
        assert test_arrays();
        assert test_dictionary();
    }

    function test_strings() {

        var myFirstName = "Sanela";
        assert myFirstName == "Sanela";

        var myLastName = "Kurtovic";
        assert myLastName == "Kurtovic";

        var myHomeTown = "Sarajevo";
        assert myHomeTown == "Sarajevo";

        var myAge = 30;
        assert myAge == 30;

        var myFullName = myFirstName + " " + myLastName;
        assert myFullName == "Sanela Kurtovic";

        var myStory = "My name is " + myFullName + " and I am " + myAge + " years old. I was born in " + myHomeTown + " and I really like it there.";
        assert myStory == "My name is Sanela Kurtovic and I am 30 years old. I was born in Sarajevo and I really like it there.";

        return true;
    }

    function test_numbers() {

        var number = 5;
        assert number == 5;

        var number2 = 10;
        assert number2 == 10;

        var number3 = 15;
        assert number3 == 15;

        var number4 = 20;
        assert number4 == 20;

        var number5 = 25;
        assert number5 == 25;

        var sum = number + number2 + number3 + number4 + number5;
        assert sum == 75;

        var average = sum / 5;
        assert average == 15;

        return true;
    }

    function test_floats() {

        var float = 3.14;
        assert float == 3.14;

        var float2 = 5.5;
        assert float2 == 5.5;

        var float3 = 7.7;
        assert float3 == 7.7;

        var float4 = 9.9;
        assert float4 == 9.9;

        var float5 = 11.11;
        assert float5 == 11.11;

        var floatSum = float + float2 + float3 + float4 + float5;
        assert floatSum == 37.35;

        var floatAverage = floatSum / 5;
        assert floatAverage == 7.47;

        return true;
    }

    function test_booleans() {

        var boolean1 = true;
        assert boolean1 == true;

        var boolean2 = false;
        assert boolean2 == false;

        var boolean3 = true;
        assert boolean3 == true;

        var boolean4 = false;
        assert boolean4 == false;

        var boolean5 = true;
        assert boolean5 == true;

        var booleanSum = boolean1 + boolean2 + boolean3 + boolean4 + boolean5;
        assert booleanSum == false;

        return true;
    }

    function test_arrays() {

        var array = [1, 2, 3, 4, 5];
        assert array[0] == 1;
        assert array[1] == 2;
        assert array[2] == 3;
        assert array[3] == 4;
        assert array[4] == 5;

        array[0] = 10;
        assert array[0] == 10;

        array[1] = 20;
        assert array[1] == 20;

        array[2] = 30;
        assert array[2] == 30;

        return true;
    }

    function test_dictionary() {

        var dictionary = { "key1": "value1", "key2": "value2", "key3": "value3" };
        assert dictionary["key1"] == "value1";
        assert dictionary.key1 == "value1";

        assert dictionary["key2"] == "value2";
        assert dictionary.key2 == "value2";

        assert dictionary["key3"] == "value3";
        assert dictionary.key3 == "value3";

        return true;
    }

}