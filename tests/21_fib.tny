class Fibonacci {

    function main() {
        var fib = new Fibonacci();
    }

    function constructor() {
        fib_quick();
    }

    function fib_quick() {
        var count = 10;
        var expected = 55;
        assert fib(count) == expected;
    }

    function fib_long() {
        var count = 20;
        var expected = 6765;
        assert fib(count) == expected;
    }

    function fib(n) {

        if n == 0 {
            return 0;
        }

        if n == 1 {
            return 1;
        }

        return fib(n - 1) + fib(n - 2);
    }
}